//O(N) solution

class Solution {
    public int maxSubArray(int[] nums) {
        int maxEndingHere=nums[0];
        int maxSum=nums[0];
        for(int i=1;i<nums.length;i++){
            maxEndingHere=Math.max(nums[i],maxEndingHere+nums[i]);
            maxSum=Math.max(maxSum,maxEndingHere);
        }
        return maxSum;
    }
}

//divide and conquer solution

